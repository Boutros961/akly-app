{
  "expo": {
    "name": "akly",
    "slug": "akly",
    "owner": "boutros961",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_akly.png",
    "scheme": "akly",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.boutros961.akly"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_akly.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.boutros961.akly"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/frigo.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/frigo.png",
          "imageWidth": 400,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
